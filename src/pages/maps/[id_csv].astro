---
import { supabase } from "../../lib/supabase";

const { params } = Astro;
const id_csv = params.id_csv;

if (!id_csv) {
  return {
    status: 400,
    body: "id_csv is required",
  };
}
import CSVMap from "../../components/svelte-v2/CSVMaps/CSVMap.svelte";
import Layout from "../../layouts/AuthLayout.astro";
const { data } = await supabase
  .from("csv_dataset")
  .select("*")
  .eq("id", id_csv)
  .single();

const csv_url = data?.csv_url;
console.log(csv_url);

if (!csv_url) {
  return {
    status: 404,
    body: "CSV not found",
  };
}
---

<Layout title="" >
  <CSVMap
    client:only
    map_config={{
      csv_url,
    }}
  />
</Layout>
